<h1>Rank dimensions</h1>
Description: <%= @decision_problem.description %>

<%= nested_form_for @decision_problem, url: save_dimension_ranks_decision_problem_path do |f| %>
  <%= f.fields_for :dimensions do |dimension_form| %>
    <%= dimension_form.text_field :rank %>
    <span><%= dimension_form.object.description %></span>
  <% end %>
  <%= f.submit %>
<% end %>
